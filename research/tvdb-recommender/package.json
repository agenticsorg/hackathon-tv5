{
  "name": "tvdb-recommender",
  "version": "1.0.0",
  "description": "Self-learning recommendation system for TheTVDB.com using AgentDB and agentic-flow",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "db:setup": "tsx src/db/setup.ts",
    "db:migrate": "npx @ruvector/postgres-cli install && tsx src/db/migrate.ts",
    "embeddings:generate": "tsx src/scripts/generate-embeddings.ts",
    "learn:train": "tsx src/scripts/train-patterns.ts",
    "workflow:start": "npx agentic-flow@alpha start",
    "ingest": "tsx src/services/tvdb-data-ingestion.ts",
    "ingest:swarm": "tsx src/scripts/swarm-ingest.ts",
    "ingest:swarm:fast": "SWARM_PARALLELISM=6 tsx src/scripts/swarm-ingest.ts",
    "optimize": "tsx -e \"import('./src/services/ruvector-optimizer.ts').then(m => new m.RuVectorOptimizer().runOptimizationCycle().then(console.log))\"",
    "learn:hyperbolic": "tsx -e \"import('./src/services/hyperbolic-ruvector.ts').then(async m => { const s = new m.HyperbolicRuVectorService(); await s.initialize(); console.log(await s.getStats()); await s.close(); })\"",
    "enable:hyperbolic": "tsx src/scripts/enable-hyperbolic.ts",
    "learn:optimize": "tsx -e \"import('./src/services/self-learning-engine.ts').then(async m => { const e = new m.SelfLearningEngine(); await e.initialize(); console.log(await e.getStats()); const r = await e.optimize(); console.log('Optimization:', r); await e.close(); })\"",
    "learn:stats": "tsx -e \"import('./src/services/self-learning-engine.ts').then(async m => { const e = new m.SelfLearningEngine(); await e.initialize(); console.log(JSON.stringify(await e.getStats(), null, 2)); await e.close(); })\"",
    "search": "tsx src/scripts/search-with-learning.ts",
    "benchmark": "tsx src/scripts/benchmark-learning.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroui/react": "^2.8.5",
    "@heroui/system": "^2.4.23",
    "@heroui/theme": "^2.4.23",
    "@ruvector/postgres-cli": "^0.2.5",
    "@xenova/transformers": "^2.17.0",
    "agentdb": "alpha",
    "agentic-flow": "alpha",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "framer-motion": "^12.23.25",
    "next": "^16.0.7",
    "node-cache": "^5.1.2",
    "pg": "^8.11.3",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.55.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "tvdb",
    "recommendation",
    "self-learning",
    "vector-database",
    "agentdb",
    "agentic-flow",
    "ai",
    "embeddings"
  ],
  "author": "",
  "license": "MIT"
}
